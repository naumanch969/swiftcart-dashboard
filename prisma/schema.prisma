generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider     = "mongodb"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Store {
  id        String   @id @map("_id")
  createdAt DateTime @db.Date
  name      String
  updatedAt DateTime @db.Date
  userId    String
}
